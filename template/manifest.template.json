{
    "name": "Vue.JS Todo Extension",
    "short_name": "Vue Todo Extension",
    "description": "A todo reminder app extension written in VueJS",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "16": "images/icon.png",
        "128": "images/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon.png",
            "38": "images/icon.png"
        },
        "default_title": "An useful todo extension",
        "default_popup": "index.html"
    },
    "omnibox": {
        "keyword": "todo"
    },
    "background": {
        "scripts": ["js/background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "storage",
        "<all_urls>"
    ]
}